<br/>
<table class= "table">
  <h1><%= @list.title %>
    <span class="pull-right">
      <%= link_to "Edit List", edit_list_path, class: "btn btn-success" %>
      <%= link_to "Delete List", @list, method: :delete, class: "btn btn-danger", data: { confirm: 'Are you sure you want to delete this list?'} %>
    </span>
  </h1>

    <tr>
      <th><h3>Completed</h3></th>
      <th><h3>Task To-Do</h3></th>
      <th><h3>Days Left</h3></th>
    </tr>
  <div class="js-items">
    <%= render @items %>
  </div>
</table>
  <div class="new-item">
    <%= render partial: 'items/form', locals: {list: @list} %>
  </div>
